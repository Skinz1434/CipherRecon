{
  "name": "cipher-recon",
  "version": "2.0.0",
  "description": "Advanced Text Intelligence & Steganography Suite",
  "main": "dist/index.js",
  "bin": {
    "cipher-recon": "./src/cli/index.js"
  },
  "scripts": {
    "start": "node src/api/server.js",
    "dev": "nodemon src/api/server.js",
    "cli": "node src/cli/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write 'src/**/*.{js,css,html}'",
    "build": "webpack --config webpack.config.js",
    "build:prod": "webpack --config webpack.prod.config.js",
    "build:complete": "node scripts/build.js",
    "serve:web": "http-server src/web -p 8080",
    "serve:dist": "http-server dist -p 8080"
  },
  "keywords": [
    "security",
    "steganography",
    "text-transformation",
    "red-team",
    "cryptography",
    "encoding",
    "api",
    "cli"
  ],
  "author": "SkinzAI",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "commander": "^11.0.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.5",
    "ora": "^5.4.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "prettier": "^3.0.3",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "http-server": "^14.1.1"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SkinzAI/CipherRecon.git"
  }
}
